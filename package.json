{
  "name": "esducad-ui",
  "version": "1.0.0",
  "description": "A web development ui kit library for React.Js by emresandikci",
  "main": "dist/index.js",
  "author": "emresandikci <me@emresandikci.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn clean && yarn install && yarn transpile",
    "build:doc": "build-storybook --docs -c .storybook -o .out",
    "build:icon": "yarn svgr ./src/static/icons -d ./src/icons --svgo-config .svgorc.json",
    "start:doc": "start-storybook --docs -p 1992",
    "transpile": "babel src/ -d dist/ --ignore src/**/*.story.js,src/static/icons/*",
    "clean": "rimraf ./dist",
    "check": "yarn lint:fix && yarn lint && yarn lint:css",
    "lint": "eslint ./src/*",
    "lint:watch": "esw ./src/* -w --color",
    "lint:css": "stylelint './src/*'",
    "lint:fix": "eslint --fix ./src/*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@styled-system/prop-types": "^5.1.5",
    "prop-types": "^15.7.2",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@svgr/cli": "^5.3.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-watch": "^6.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.0.1",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "keywords": [
    "ui",
    "ui-kit",
    "component",
    "components",
    "design",
    "css",
    "framework",
    "frontend",
    "react",
    "react-component",
    "styled-component",
    "styled-system",
    "esducad",
    "esducad-ui"
  ]
}
